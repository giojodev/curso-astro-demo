---
import CardLaunche from "./CardLaunche.astro";
import { getLatestLaunches } from "../services/spacex.services";

const launches = await getLatestLaunches();
---

<div class="grid gap-8 sm:grid-cols-2 lg:grid-cols-3">
{
    launches.map(({id,links,details,flight_number:flightNumer,success})=>
        <CardLaunche 
        id={id}
        img={links.patch.small}
        details={details}
        flightNumer={flightNumer}
        success={success}
        />
    )
}
</div>